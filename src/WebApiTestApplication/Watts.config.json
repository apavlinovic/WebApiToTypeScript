{
  "WebApiModuleFileName": "./bin/WebApiTestApplication.dll",

  "GenerateEndpointsReturnTypes": true,

  "EndpointsOutputDirectory": "./Scripts/Endpoints",
  "EndpointsSupportCaching": true,
  "EndpointsFileName": "Endpoints.ts",
  "EndpointsNamespace": "Endpoints",

  "GenerateService": true,
  "ServiceOutputDirectory": "./Scripts/Endpoints",
  "ServiceFileName": "Service.ts",
  "ServiceNamespace": "Endpoints",

  "GenerateEnums": true,
  "GenerateEnumDescriptions": true,
  "EnumsOutputDirectory": "./Scripts/Enums",
  "EnumsFileName": "Enums.ts",
  "EnumsNamespace": "Enums",
  "EnumMatches": [
    {
      "Match": "WebApiTestApplication.Enums.*$"
    }
  ],

  "GenerateInterfaces": true,
  "InterfacesOutputDirectory": "./Scripts/Interfaces",
  "InterfacesFileName": "Interfaces.ts",
  "InterfacesNamespace": "Interfaces",
  "InterfaceMatches": [
    {
      "Match": "WebApiTestApplication.Controllers.*Chain.*$"
    }
  ],

  "ScanOtherModules": true,
  "WriteNamespaceAsModule": false,

  "TypeMappings": [
    {
      "WebApiTypeName": "EncryptedInt",
      "TypeScriptTypeName": "string",
      "TreatAsAttribute": true,
      "TreatAsConstraint": true
    },
    {
      "WebApiTypeName": "System.Web.Http.IHttpActionResult",
      "TypeScriptTypeName": "any"
    }
  ]
}